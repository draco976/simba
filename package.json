{
    "name": "verifier",
    "publisher": "phoevos",
    "displayName": "Copilot Chat @verifier",
    "description": "A Github Copilot Chat agent that does its best to verify Python code suggestions",
    "repository": {
        "type": "git",
        "url": "https://github.com/phoevos/vscode-extension-verifier"
    },
    "version": "1.1.0",
    "engines": {
        "vscode": "^1.88.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished",
        "onCommand:extension.checkUnpushedCommits"
    ],
    "contributes": {
        "commands": [
            {
                "command": "extension.checkUnpushedCommits",
                "title": "Check Unpushed Git Commits"
            }
        ]
    },
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "lint": "eslint src --ext .ts",
        "update-vscode-dts": "cd src && npx --yes vscode-dts@latest dev -f"
    },
    "devDependencies": {
        "@types/json-schema": "^7.0.15",
        "@types/node": "^20.17.30",
        "@types/vscode": "^1.99.1",
        "@typescript-eslint/eslint-plugin": "^7.7.0",
        "@typescript-eslint/parser": "^7.7.0",
        "eslint": "^8.57.0",
        "eslint-plugin-prettier": "^5.1.3",
        "prettier": "3.2.5",
        "tslint": "^6.1.3",
        "typescript": "^4.0.3",
        "simple-git": "3.27.0"
    },
    "dependencies": {
        "@openai/openai": "npm:@jsr/openai__openai@^4.96.0",
        "axios": "^1.9.0",
        "fs": "0.0.1-security",
        "path": "^0.12.7",
        "openai": "^4.96.0"
    }
}
